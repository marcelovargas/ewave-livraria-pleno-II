@model IEnumerable<Entities.Emprestimo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DFIm)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.Letor)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Livro)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DFIm)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Letor.Id)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Livro.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
